{
    "name": "dillingham/nova-items-field",
    "description": "Nova field to handle array columns",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0 || >=8"
    },
    "autoload": {
        "psr-4": {
            "NovaItemsField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaItemsField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
